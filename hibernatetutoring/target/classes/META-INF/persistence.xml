<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
    xmlns="http://xmlns.jcp.org/xml/ns/persistence" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="pu" transaction-type="RESOURCE_LOCAL">
 
        <!-- Define the object that should be persisted in the database -->
    <class>h11dt.sessionfactory_entitymanager.Students12</class>
    <class>hibernate_review.Students</class>
   
    <properties>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.driver.OracleDriver"/>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521/ORCLCDB.localdomain"/>
      <property name="javax.persistence.jdbc.user" value="dummy"/>
      <property name="javax.persistence.jdbc.password" value="dummy"/>
    </properties>
  </persistence-unit>
</persistence>